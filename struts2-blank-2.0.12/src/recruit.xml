<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
<!-- jsp 파일 연결 -->
    <package name="recruit" namespace="/recruit" extends="struts-default">
        <action name="recruitInsert" class="recruit.action.RecruitInsertAction">
        	<result name="success" type="dispatcher">
        		/jsp/recruitInfo/recruit/recruitJsonData.jsp
          	</result>
        </action>  
        
        <action name="recruitSelect" class="recruit.action.RecruitSelectAction">
        	<result name="success" type="dispatcher">
        		/jsp/recruitInfo/recruit/recruitSelectData.jsp
          	</result>
        </action>     
        
        
    </package>
</struts>
