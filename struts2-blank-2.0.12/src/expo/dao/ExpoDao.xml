<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="expo">
	<typeAlias alias="ExpoBean" type="expo.bean.ExpoBean" />
	<insert id="insertExpo" parameterClass="ExpoBean">
		INSERT INTO TEST01
		VALUES(
		#id#,#title#,#company#,#regdate#,
		#contents#,#pass#,'1',#fileFileName#)
	</insert>
	<select parameterClass="java.lang.String" id="selectExpo"
		resultClass="ExpoBean"> SELECT COMPANY,TITLE,REGDATE,NO,fileFileName FROM TEST01
	</select>
</sqlMap>











