<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="expo">
	<typeAlias alias="ExpoBean" type="expo.bean.ExpoBean" />
<!-- 	EXP_NO	MEM_ID	EXP_TITLE	EXP_DATE	EXP_FILENAME	 -->
	<insert id="insertExpo" parameterClass="ExpoBean">
		INSERT INTO EXPO
		VALUES(
		EXPO_SEQ.NEXTVAL,'admin1',#exp_title#,#exp_date#,#fileFileName#	)
	</insert>
	<select  id="selectExpo"
		resultClass="ExpoBean"> SELECT EXP_NO,MEM_ID,EXP_TITLE,EXP_DATE,EXP_FILENAME FROM EXPO
	</select>
	<select id="selectExpoInfo" resultClass="ExpoBean"
		parameterClass="java.lang.Integer">
		SELECT
		EXP_NO,MEM_ID,EXP_TITLE,EXP_DATE,EXP_FILENAME
		FROM
		EXPO
		WHERE
		EXP_NO=#VALUE#
	</select>
</sqlMap>











